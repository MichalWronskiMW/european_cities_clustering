import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
from pathlib import Path

pd.set_option('display.max_columns', None)


df = pd.read_csv('../data/preprocessed/preprocessed_data.csv')
df.head()


df.describe().T





# 1️⃣ Tworzymy przedziały populacji
bins = [0, 50_000, 100_000, 250_000, 500_000, 1_000_000, np.inf]
labels = ['0-50k', '50-100k', '100-250k', '250-500k', '500-1000k', '1000k+']

df['pop_group'] = pd.cut(df['Population'], bins=bins, labels=labels)

# 2️⃣ Grupujemy po przedziałach i liczymy % braków w każdej kolumnie
missing_matrix = df.groupby('pop_group').apply(lambda x: x.isna().mean() * 100)

# 3️⃣ Opcjonalnie usuwamy kolumnę 'pop_group' z wyników
missing_matrix = missing_matrix.drop(columns=['pop_group'], errors='ignore')

# 4️⃣ Wyświetlamy wynik
print(missing_matrix)




















